//1. Print odd numbers in an array 
// anonymous function : 
let arr = [];
let odd = function (value) {
    value.forEach((i) => {
        if (i % 2 !== 0) {
            arr.push(i);
        }
    })
    return empty.join(",")
}
console.log(odd([1, 2, 3, 4, 5, 6,7]));

//iIFE () method:
(function (value) {
    let array = [];
    value.forEach((i) => {
        if (i % 2 !== 0) {
            empty.push(i);
        }
    });
    console.log(array.join(",")); 
})([1, 2, 3, 4, 5, 6,7])
