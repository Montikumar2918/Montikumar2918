//IIFE function using : ()()
(function (arr) {
    arr.forEach((odd) => {
        if (odd % 2 != 0) {
            console.log("odd numbers:", odd);
        };
    });
}
)([1,2, 3, 4, 5, 6]);


// anonyous function:

let arr = function (odd) {
    odd.forEach((key) => {
        if (key % 2 != 0) {
            console.log("odd number:", key);
        };
    });


};
arr([1,2, 3, 4, 5, 6]);

